{
	"name": "@lab1095/n8n-nodes-sharepoint-excel",
	"version": "0.2.0",
	"description": "n8n node to read and write Excel files stored in SharePoint (bypasses WAC token issues)",
	"license": "MIT",
	"homepage": "https://github.com/lab1085/n8n-nodes-sharepoint-excel",
	"keywords": [
		"n8n-community-node-package",
		"n8n",
		"sharepoint",
		"excel",
		"microsoft",
		"graph-api"
	],
	"author": {
		"name": "juanpujol"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/lab1085/n8n-nodes-sharepoint-excel.git"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"format": "prettier --write .",
		"lint": "tsc --noEmit && n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"prepublishOnly": "n8n-node prerelease",
		"prepare": "husky",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"strict": false,
		"credentials": [
			"dist/credentials/MicrosoftGraphOAuth2Api.credentials.js"
		],
		"nodes": [
			"dist/nodes/SharePointExcel/SharePointExcel.node.js"
		]
	},
	"devDependencies": {
		"@commitlint/cli": "^20.4.0",
		"@commitlint/config-conventional": "^20.4.0",
		"@n8n/node-cli": "*",
		"@types/node": "^22.0.0",
		"eslint": "9.32.0",
		"husky": "^9.1.7",
		"prettier": "^3.3.0",
		"release-it": "^19.0.4",
		"typescript": "5.9.2",
		"vitest": "^4.0.18"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	},
	"dependencies": {
		"exceljs": "^4.4.0"
	}
}
